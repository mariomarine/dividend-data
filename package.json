{
  "name": "dividend-data",
  "version": "1.0.0",
  "description": "Dividend Tracker/Analysis Tool",
  "author": "mariomarine <rwbleo@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.5.0",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "nuxt": "^2.0.0",
    "vue-jest": "^3.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
	"moduleNameMapper": {
	  "^@/(.*)$": "<rootDir>/$1"
	}
  }
}
